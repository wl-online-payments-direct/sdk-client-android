<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!--
    Suppress vulnerabilities in transitive build/test dependencies that don't affect
    the runtime library or are false positives. These are not shipped with the SDK.
    -->

    <!-- gRPC - Transitive dependency from Android build tools -->
    <suppress>
        <notes>CVE-2023-44487: HTTP/2 Rapid Reset - Not used in Android SDK runtime, only in build tools</notes>
        <packageUrl regex="true">^pkg:maven/io\.grpc/.*@1\.57\.2$</packageUrl>
        <cve>CVE-2023-44487</cve>
    </suppress>

    <!-- Netty - Transitive dependency from gRPC/build tools -->
    <suppress>
        <notes>Netty vulnerabilities - Not used in Android SDK runtime, only in build tools</notes>
        <packageUrl regex="true">^pkg:maven/io\.netty/.*@4\.1\.(93|110)\.Final$</packageUrl>
        <cve>CVE-2023-44487</cve>
        <cve>CVE-2025-24970</cve>
        <cve>CVE-2025-58057</cve>
        <cve>CVE-2025-55163</cve>
        <cve>CVE-2025-58056</cve>
        <cve>CVE-2024-47535</cve>
        <cve>CVE-2025-25193</cve>
        <cve>CVE-2023-34462</cve>
        <cve>CVE-2024-29025</cve>
    </suppress>

    <!-- Protobuf - Transitive dependency from build tools -->
    <suppress>
        <notes>CVE-2024-7254: Protobuf - Not used in Android SDK runtime, only in build tools</notes>
        <packageUrl regex="true">^pkg:maven/com\.google\.protobuf/.*@3\.(22\.3|24\.4|25\.5)$</packageUrl>
        <cve>CVE-2024-7254</cve>
    </suppress>

</suppressions>
